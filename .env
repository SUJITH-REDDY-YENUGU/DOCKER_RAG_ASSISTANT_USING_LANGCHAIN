# .env file for Docker RAG Assistant

# Groq API Key (required)
GROQ_API_KEY=your_api_key_here

# Embedding model (default: all-MiniLM-L6-v2)
EMBEDDING_MODEL=all-MiniLM-L6-v2

# Vector database path (ChromaDB storage)
CHROMA_DB_PATH=./data/chroma_db

# Chunking parameters
CHUNK_SIZE=500
CHUNK_OVERLAP=50

# u can use by loading these environment variables in any section you can remove already set  & use these
